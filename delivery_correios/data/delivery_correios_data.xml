<odoo>
    <data noupdate="1">
        <record id="product_product_delivery_correios" model="product.product">
            <field name="name">Correios</field>
            <field name="default_code">correios</field>
            <field name="type">service</field>
            <field name="categ_id" ref="delivery.product_category_deliveries"/>
            <field name="sale_ok" eval="False"/>
            <field name="purchase_ok" eval="False"/>
            <field name="list_price">0.0</field>
        </record>

        <record id="delivery_carrier_correios" model="delivery.carrier">
            <field name="name">Correios</field>
            <field name="delivery_type">correios</field>
            <field name="product_id" ref="product_product_delivery_correios"/>
        </record>
    </data>
</odoo>
